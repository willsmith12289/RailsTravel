<tr>
  <td>
    <%= marker.title %>
  </td>
  <td>
    <%= marker.address %>
  </td>
  <td>
    <%= marker.latitude %>
  </td>
  <td>
    <%= marker.longitude %>
  </td>
  <td>
    <%= marker.info %>
  </td>
  <td>
    <%= marker.map_id %>
  </td>
  <td class="place">
    <%=marker.place_id %>
  </td>
  <td>
    <%= link_to 'Show', marker_path(marker.id), class: "btn btn-default" %>
  </td>
  <td>
    <%= link_to 'Destroy', marker_path(marker), controller: 'markers', method: :delete, class: "btn btn-default" %>
  </td>
  <td class="place">
    <%=marker.id %>
  </td>
</tr>